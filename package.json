{
  "name": "gitga",
  "version": "0.0.0",
  "description": "A graphical git interface",
  "main": "src/main.js",
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha",
    "coverage": "cross-env NODE_ENV=test nyc mocha",
    "coverage-report": "cross-env NODE_ENV=test nyc report --reporter=text-lcov > coverage.lcov",
    "build": "webpack --mode production",
    "serve": "concurrently --kill-others \"npm run serve:renderer\" \"npm run serve:electron\" ",
    "serve:renderer": "webpack-dev-server --mode development --hot --port 8081",
    "serve:electron": "electron . --remote-debugging-port 9222"
  },
  "author": "Erik Moldtmann",
  "license": "GPL-3.0",
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist/*",
      "webpack.config.js"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/mocha": "^5.0.0",
    "@types/promise.prototype.finally": "^2.0.2",
    "@types/react": "^16.0.41",
    "@types/react-bootstrap": "^0.32.5",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/sinon": "^4.3.0",
    "bootstrap-less": "^3.3.8",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "electron": "^1.8.4",
    "electron-connect": "^0.6.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "istanbul": "^0.4.5",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "mocha": "^5.0.5",
    "nyc": "^11.4.1",
    "react": "^16.2.0",
    "react-component-octicons": "^1.5.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sinon": "^4.4.4",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.3",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1",
    "webpack-stream": "^4.0.2",
    "yarn": "^1.5.1"
  },
  "dependencies": {}
}
