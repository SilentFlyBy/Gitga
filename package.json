{
  "name": "gitga",
  "version": "0.0.0",
  "description": "A graphical git interface",
  "main": "dist/main/main.js",
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "scripts": {
    "compile": "electron-webpack",
    "test": "cross-env NODE_ENV=test electron-mocha",
    "coverage": "cross-env NODE_ENV=test istanbul cover electron-mocha -- --renderer --R spec",
    "coverage-report": "cross-env NODE_ENV=test nyc report --reporter=text-lcov > coverage.lcov",
    "build": "run-p build:renderer build:electron",
    "build:renderer": "webpack --mode production --config webpack.renderer.js --display-error-details",
    "build:electron": "webpack --mode production --config webpack.electron.js --display-error-details",
    "dev": "run-s build:electron serve",
    "dv": "electron-webpack dev",
    "serve": "concurrently --kill-others \"npm run serve:renderer\" \"npm run serve:electron\" ",
    "serve:renderer": "webpack-dev-server --mode development --hot --port 8081 --config webpack.renderer.js",
    "serve:electron": "electron . --remote-debugging-port 9222",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "electronWebpack": {
    "commonSourceDirectory": "src/core",
    "main": {
      "sourceDirectory": "src/electron"
    },
    "renderer": {
      "sourceDirectory": "src/browser"
    }
  },
  "author": "Erik Moldtmann",
  "license": "GPL-3.0",
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist/*",
      "webpack.config.js"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true
  },
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "devDependencies": {
    "@alienfast/i18next-loader": "^1.0.15",
    "@types/chai": "^4.1.2",
    "@types/electron-json-storage": "^4.0.0",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/i18next": "^8.4.3",
    "@types/mocha": "^5.0.0",
    "@types/nodegit": "^0.18.6",
    "@types/promise.prototype.finally": "^2.0.2",
    "@types/react": "^16.0.41",
    "@types/react-dom": "^16.0.4",
    "@types/react-i18next": "^7.3.2",
    "@types/react-redux": "^5.0.15",
    "@types/react-transition-group": "^2.0.7",
    "@types/redux-logger": "^3.0.5",
    "@types/sinon": "^4.3.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "electron-connect": "^0.6.3",
    "electron-json-storage": "^4.0.2",
    "electron-mocha": "^6.0.1",
    "electron-rebuild": "^1.7.3",
    "electron-webpack": "^2.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "i18next": "^10.6.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.6.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "mocha": "^5.0.5",
    "node-loader": "^0.6.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "react": "^16.2.0",
    "react-component-octicons": "^1.5.0",
    "react-dom": "^16.2.0",
    "react-i18next": "^7.5.1",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.3.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sinon": "^4.4.4",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1",
    "webpack-stream": "^4.0.2",
    "yarn": "^1.5.1"
  },
  "dependencies": {
    "nodegit": "^0.21.2",
    "source-map-support": "^0.5.4"
  }
}
